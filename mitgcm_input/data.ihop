 IHOP_rr=2542.0,
# e: eigenrays; G: Hat spreading
 IHOP_runopt='eG',
 IHOP_nalpha=4,
 IHOP_alpha=-20.0, 20.0,
 &

 &IHOP_PARM03
 IHOP_interpfile = 'bcg_ihop-grid',
#ihop_iter = 1,2,3,4,5,6,7,8,
 ihop_iter = 6,7,8,

 &

 &IHOP_COST_NML
